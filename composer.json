{
  "name": "rrortega/trustpilot-review-collector",
  "description": "TrustpilotReviewCollector is a PHP class designed to fetch and parse profile business data and reviews from Trustpilot.com",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Rolando Rodriguez Ortega",
      "email": "rolymayo11@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0|^8.1",
    "symfony/dom-crawler": "^4.3",
    "symfony/http-client": "^4.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6.5"
  },
  "autoload": {
    "psr-4": {
      "RRO\\Review\\Collector\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "RRO\\Review\\Collector\\": "src/"
    }
  },
  "minimum-stability": "stable"
}
